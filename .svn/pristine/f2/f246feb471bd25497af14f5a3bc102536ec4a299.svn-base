<?xml version="1.0" encoding="Windows-1251"?>
<!-- nn.xsd -->
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:annotation>
		<xsd:documentation xml:lang="ru">XML-подсхема для проверки документов - Типы данных для СЗВ-4 </xsd:documentation>
	</xsd:annotation>
	<xsd:include schemaLocation="Types.XSD"/>
	<!--Тип Тариф -->
	<xsd:complexType name="ТипТариф">
		<xsd:sequence>
			<xsd:element name="КодКатегории">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="4"/>
						<xsd:enumeration value="НР"/>
						<xsd:enumeration value="СХ"/>
						<xsd:enumeration value="СДП"/>
						<xsd:enumeration value="ДП"/>
						<xsd:enumeration value="БЕЗР"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="КодДополнительногоТарифа" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="4"/>
						<xsd:enumeration value="АВИА"/>
						<xsd:enumeration value=""/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="НазваниеКатегории" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="20"/>
						<xsd:enumeration value="НАЕМ/РАБОТНИК"/>
						<xsd:enumeration value="С/Х ПРЕДПРИЯТИЕ"/>
						<xsd:enumeration value="С/Х ОРГАНИЗАЦИЯ"/>
						<xsd:enumeration value="СТАЖ/ДОБРОВ/ПРАВООТН"/>
						<xsd:enumeration value="ДОБРОВ/ПРАВООТН"/>
						<xsd:enumeration value="БЕЗРАБОТНЫЙ"/>
						<xsd:enumeration value=""/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="НазваниеДополнительногоТарифа" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="20"/>
						<xsd:enumeration value="ЧЛЕН ЛЭВС"/>
						<xsd:enumeration value=""/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- Тип Взносы   -->
	<xsd:complexType name="ТипСтраховыеВзносы">
		<xsd:sequence minOccurs="0">
			<xsd:element name="Страховые" type="ТипДеньги" minOccurs="0"/>
			<xsd:element name="Накопительные" type="ТипДеньги" minOccurs="0"/>
			<xsd:element name="Дополнительные" type="ТипДеньги" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- Тип ВыработкаКалендарная  -->
	<xsd:complexType name="ТипВыработкаКалендарная">
		<xsd:sequence minOccurs="0">
			<xsd:element name="ВсеГоды" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="99"/>
						<xsd:totalDigits value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="ВсеМесяцы" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="12"/>
						<xsd:totalDigits value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="ВсеДни" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="29"/>
						<xsd:totalDigits value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- Тип ВыработкаВчасах  -->
	<xsd:complexType name="ТипВыработкаВчасах">
		<xsd:sequence>
			<xsd:element name="Часы" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="0"/>
						<xsd:totalDigits value="5"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="Минуты" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="59"/>
						<xsd:totalDigits value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- Тип ТерриториальныеУсловия  -->
	<xsd:complexType name="ТипТерриториальныеУсловия">
		<xsd:sequence>
			<xsd:element name="ОснованиеТУ">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="3"/>
						<xsd:enumeration value="РКС"/>
						<xsd:enumeration value="МКС"/>
						<xsd:enumeration value="Ч31"/>
						<xsd:enumeration value="Ч33"/>
						<xsd:enumeration value="Ч34"/>
						<xsd:enumeration value="Ч35"/>
						<xsd:enumeration value="Ч36"/>
						<xsd:enumeration value=""/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="Коэффициент" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:fractionDigits value="2"/>
						<xsd:totalDigits value="3"/>
						<xsd:minInclusive value="0.00"/>
						<xsd:maxInclusive value="2.00"/>
						<xsd:pattern value="\d{1}\.\d{2}"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- Тип ОсобыеУсловияТруда  -->
	<xsd:complexType name="ТипОсобыеУсловияТруда">
		<xsd:sequence>
			<xsd:element name="ОснованиеОУТ" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="6"/>
						<xsd:enumeration value="27-1"/>
						<xsd:enumeration value="27-2"/>
						<xsd:enumeration value="27-3"/>
						<xsd:enumeration value="27-4"/>
						<xsd:enumeration value="27-5"/>
						<xsd:enumeration value="27-6"/>
						<xsd:enumeration value="27-7"/>
						<xsd:enumeration value="27-8"/>
						<xsd:enumeration value="27-9"/>
						<xsd:enumeration value="27-10"/>
						<xsd:enumeration value="28-ОС"/>
						<xsd:enumeration value="28-ПЖ"/>
						<xsd:enumeration value="27-ОС"/>
						<xsd:enumeration value="27-ПЖ"/>
						<xsd:enumeration value="28-СЕВ"/>
						<xsd:enumeration value=""/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="ПозицияСписка" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="20"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- Тип ИсчисляемыйСтаж  -->
	<xsd:complexType name="ТипИсчисляемыйСтаж">
		<xsd:sequence>
			<xsd:element name="ОснованиеИС" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="7"/>
						<xsd:enumeration value="СЕЗОН"/>
						<xsd:enumeration value="УИК104"/>
						<xsd:enumeration value="ВОДОЛАЗ"/>
						<xsd:enumeration value="ЛЕПРО"/>
						<xsd:enumeration value="ИТК38"/>
						<xsd:enumeration value="ПОЛЕ"/>
						<xsd:enumeration value=""/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:choice>
				<xsd:element name="ВыработкаВчасах" type="ТипВыработкаВчасах" minOccurs="0"/>
				<xsd:element name="ВыработкаКалендарная" type="ТипВыработкаКалендарная" minOccurs="0"/>
			</xsd:choice>
		</xsd:sequence>
	</xsd:complexType>
	<!-- Тип ВыслугаЛет  -->
	<xsd:complexType name="ТипВыслугаЛет">
		<xsd:sequence>
			<xsd:element name="ОснованиеВЛ" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="8"/>
						<xsd:enumeration value="27-11ГР"/>
						<xsd:enumeration value="27-11ВП"/>
						<xsd:enumeration value="27-12"/>
						<xsd:enumeration value="27-СП"/>
						<xsd:enumeration value="27-ПД"/>
						<xsd:enumeration value="27-ПДРК"/>
						<xsd:enumeration value="27-ГД"/>
						<xsd:enumeration value="27-СМ"/>
						<xsd:enumeration value="27-СП"/>
						<xsd:enumeration value="27-ГДХР"/>
						<xsd:enumeration value="27-СМХР"/>	
						<xsd:enumeration value="28-СП"/>
						<xsd:enumeration value="28-ПД"/>
						<xsd:enumeration value="28-ПДРК"/>
						<xsd:enumeration value="28-ГД"/>
						<xsd:enumeration value="28-СМ"/>
						<xsd:enumeration value="28-ГДХР"/>
						<xsd:enumeration value="28-СМХР"/>
						<xsd:enumeration value="ТВОРЧ15"/>
						<xsd:enumeration value="ТВОРЧ20"/>
						<xsd:enumeration value="ТВОРЧ25"/>
						<xsd:enumeration value="ТВОРЧ30"/>
						<xsd:enumeration value="САМОЛЕТ"/>
						<xsd:enumeration value="СПЕЦАВ"/>
						<xsd:enumeration value="СПАСАВ"/>
						<xsd:enumeration value="УЧЛЕТ"/>
						<xsd:enumeration value="ВЫСШПИЛ"/>
						<xsd:enumeration value="НОРМАПР"/>
						<xsd:enumeration value="НОРМСП"/>
						<xsd:enumeration value="РЕАКТИВН"/>
						<xsd:enumeration value="ЛЕТРАБ"/>
						<xsd:enumeration value="ЛЕТИСП"/>
						<xsd:enumeration value="ОПЫТИСП"/>
						<xsd:enumeration value="ИСПКЛС1"/>
						<xsd:enumeration value="ИТСИСП"/>
						<xsd:enumeration value="ИТСМАВ"/>
						<xsd:enumeration value="ИНСПЕКТ"/>
						<xsd:enumeration value="27-14"/>
						<xsd:enumeration value="27-15"/>
						<xsd:enumeration value=""/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:choice>
				<xsd:element name="ВыработкаВчасах" type="ТипВыработкаВчасах" minOccurs="0"/>
				<xsd:element name="ВыработкаКалендарная" type="ТипВыработкаКалендарная" minOccurs="0"/>
			</xsd:choice>
			<xsd:element name="КодИсчисления" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value=""/>
						<xsd:enumeration value="463"/>
						<xsd:enumeration value="1067"/>
						<xsd:enumeration value="781"/>
						<xsd:enumeration value="464"/>
						<xsd:enumeration value="1066"/>
						<xsd:enumeration value="0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="ДоляСтавки" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0.00"/>
						<xsd:maxInclusive value="2.00"/>
						<xsd:fractionDigits value="2"/>
						<xsd:totalDigits value="3"/>
						<xsd:pattern value="\d{1}\.\d{2}"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- Тип ОсобенностиУчета  -->
	<xsd:complexType name="ТипОсобенностиУчета">
		<xsd:sequence>
			<xsd:element name="ТерриториальныеУсловия" type="ТипТерриториальныеУсловия" minOccurs="0"/>
			<xsd:element name="ОсобыеУсловияТруда" type="ТипОсобыеУсловияТруда" minOccurs="0"/>
			<xsd:element name="ИсчисляемыйСтаж" type="ТипИсчисляемыйСтаж" minOccurs="0"/>
			<xsd:element name="ДекретДети" type="ТипДекретДети" minOccurs="0"/>
			<xsd:element name="ВыслугаЛет" type="ТипВыслугаЛет" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="ТипТипСведений">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="15"/>
			<xsd:enumeration value="ИСХОДНАЯ"/>
			<xsd:enumeration value="ПЕНСИОННАЯ"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ТипВидКорректировки">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="20"/>
			<xsd:enumeration value="КОРРЕКТИРУЮЩАЯ"/>
			<xsd:enumeration value="ОТМЕНЯЮЩАЯ"/>
			<xsd:enumeration value=""/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ТипДекретДети">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="8"/>
			<xsd:enumeration value="ДЕКРЕТ"/>
			<xsd:enumeration value="ДЕТИ"/>
			<xsd:enumeration value="ДОГОВОР"/>
			<xsd:enumeration value="УВПЕРИОД"/>
			<xsd:enumeration value="ДЛОТПУСК"/>
			<xsd:enumeration value=""/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ТипВидДеятельности">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="10"/>
			<xsd:enumeration value="РАБОТА"/>
			<xsd:enumeration value="ДВСТО"/>
			<xsd:enumeration value="СЛУЖБА"/>
			<xsd:enumeration value="РАБЗАГР"/>
			<xsd:enumeration value="СЛПРИЗ"/>
			<xsd:enumeration value="СОЦСТРАХ"/>
			<xsd:enumeration value="НЕРАБМАТЬ"/>
			<xsd:enumeration value="УХОД-ДЕТИ"/>
			<xsd:enumeration value="УХОД-ИНВД"/>
			<xsd:enumeration value="ТРУДЛИЦО"/>
			<xsd:enumeration value="УХОД-ВИЧ"/>
			<xsd:enumeration value="РЕАБИЛИТ"/>
			<xsd:enumeration value="ПРОФЗАБ"/>
			<xsd:enumeration value="ПЕРЕСМОТР"/>
			<xsd:enumeration value="ПРОВОВ"/>
			<xsd:enumeration value="РАБСВПК"/>
			<xsd:enumeration value="РАБВОВ"/>
			<xsd:enumeration value="СЛДАПБЛ"/>
			<xsd:enumeration value="ИЗЛВТР"/>
			<xsd:enumeration value="РАБЛОК"/>
			<xsd:enumeration value="СЛПРОЖ"/>
			<xsd:enumeration value="СТРАХБЛ"/>
			<xsd:enumeration value="ПРЗАГР"/>
			<xsd:enumeration value="ПРБЛОК"/>
			<xsd:enumeration value="ПРКОНЦ"/>
			<xsd:enumeration value="БЕЗР"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="ТипЗаработок">
		<xsd:sequence>
			<xsd:element name="ТипСтроки">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="ИТОГО"/>
						<xsd:enumeration value="МЕСЯЦ"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="Год" type="xsd:gYear" minOccurs="0"/>
			<xsd:element name="Месяц" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:unsignedLong">
						<xsd:minInclusive value="1"/>
						<xsd:maxInclusive value="12"/>
						<xsd:totalDigits value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="НачисленоВсего" type="ТипДеньги" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ТипВсеПериодыРаботы">
		<xsd:sequence>
			<xsd:element name="Количество" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="99"/>
						<xsd:totalDigits value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="ПериодРаботы" minOccurs="0" maxOccurs="99">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="ОсновнаяСтрока">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="НомерСтроки">
										<xsd:simpleType>
											<xsd:restriction base="xsd:integer">
												<xsd:minInclusive value="1"/>
												<xsd:totalDigits value="3"/>
												<xsd:maxInclusive value="99"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="ДатаНачалаПериода" type="ТипДата"/>
									<xsd:element name="ДатаКонцаПериода" type="ТипДата"/>
									<xsd:element name="ОсобенностиУчета" type="ТипОсобенностиУчета" minOccurs="0"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="ДополнительныеСтроки" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="Количество">
										<xsd:simpleType>
											<xsd:restriction base="xsd:integer">
												<xsd:minInclusive value="1"/>
												<xsd:maxInclusive value="10"/>
												<xsd:totalDigits value="2"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="ОсобенностиУчета" type="ТипОсобенностиУчета" maxOccurs="10"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ТипОтчетныйПериод">
		<xsd:sequence>
			<xsd:element name="Квартал">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:totalDigits value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="Год">
				<xsd:simpleType>
					<xsd:restriction base="xsd:gYear">
						<xsd:minInclusive value="1996"/>
						<xsd:maxInclusive value="2001"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="Название" type="Тип30Н" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ТипДоговор">
		<xsd:sequence>
			<xsd:element name="Номер" type="ТипНомер" minOccurs="0"/>
			<xsd:element name="ДатаЗаключения" type="ТипДата"/>
			<xsd:element name="КодВидаВыплаты">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="6"/>
						<xsd:enumeration value="АВАНС"/>
						<xsd:enumeration value="ПРОЧЕЕ"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="НазваниеВидаВыплаты" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="20"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ТипСтрокаЗаработка">
		<xsd:sequence>
			<xsd:element name="ТипСтроки">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="5"/>
						<xsd:enumeration value="МЕСЯЦ"/>
						<xsd:enumeration value="ИТОГО"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="Месяц" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="1"/>
						<xsd:maxInclusive value="12"/>
						<xsd:totalDigits value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="НачисленоВсего" type="ТипДеньги"/>
			<xsd:element name="НачисленоПоБольничным" type="ТипДеньги" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
