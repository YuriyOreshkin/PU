<?xml version="1.0" encoding="Windows-1251"?>
<xs:schema version="1.0"   xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">

	<xs:annotation>
		<xs:documentation xml:lang="ru">XML-подсхема для проверки документов - Типы данных</xs:documentation>
	</xs:annotation>
	<xs:include schemaLocation="Типы.XSD"/>
	<xs:include schemaLocation="Исходящие/ТипыИСХОДЯЩИЕ_ДОКУМЕНТЫ.XSD"/>
	<xs:include schemaLocation="Входящие/ТипыВХОДЯЩИЕ_ДОКУМЕНТЫ.XSD"/>
	<xs:include schemaLocation="Исходящие/ТипыАДИ_5.XSD"/>

	
	<xs:complexType name="ТипЗаголовокФайла">
		<xs:sequence>
			<xs:element name="ВерсияФормата" type="xs:string" fixed="07.00">
				<xs:annotation>
					<xs:documentation>
						Версия используемого для документа формата, например "07.00"
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ТипФайла" type="xs:string" fixed="ВНЕШНИЙ">
				<xs:annotation>
					<xs:documentation>
						Тип файла по отношению К ПФР, например ВНЕШНИЙ						
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ПрограммаПодготовкиДанных">
				<xs:annotation>
					<xs:documentation>
						Название и версия программного средства используемого для подготовки файла с данными
					</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="НазваниеПрограммы">
							<xs:annotation>
								<xs:documentation>
									Названия программного средства используемого для подготовки файла с данными
								</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:normalizedString">
									<xs:minLength value="1"/>
									<xs:maxLength value="40"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Версия">
							<xs:annotation>
								<xs:documentation>
									Версия программного средства используемого для подготовки файла с данными
								</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:normalizedString">
									<xs:minLength value="1"/>
									<xs:maxLength value="10"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ИсточникДанных">
				<xs:annotation>
					<xs:documentation>
						Источник представивший файлы с данными
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="СТРАХОВАТЕЛЬ"/>
						<xs:enumeration value="ТРАНСФЕР-АГЕНТ"/>
						<xs:enumeration value="ОПФР-ЛИЧНО"/>
						<xs:enumeration value="ОПФР-ПРОЧЕЕ"/>
						<xs:enumeration value="НПФ"/>
						<xs:enumeration value="ПФР"/>
						<xs:enumeration value="ОРГАН_ЗАГС"/>
						<xs:enumeration value="ПОРТАЛ ГОСУСЛУГ"/>
						<xs:enumeration value="АГЕНТ ПОРТАЛА ПФР"/>
						<xs:enumeration value="МФЦ"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ТипПачкаВходящихДокументов">
			<xs:annotation>
			<xs:documentation>Абстрактный блок «ПачкаВходящихДокументов». Обязательные атрибуты блока Пачка входящих документов: «Окружение», «Стадия»</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Окружение" use="required">
			<xs:annotation>
				<xs:documentation>Для файлов, представляемых страхователями, допускается только значение «В составе файла»</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Единичный запрос"/>
					<xs:enumeration value="В составе файла"/>
					<xs:enumeration value="В составе выписки"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Стадия" use="required">
			<xs:annotation>
				<xs:documentation>Для файлов, представляемых страхователями, допускается только значение «До обработки»</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="До обработки"/>
					<xs:enumeration value="После обработки"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>

	<xs:complexType name="ТипПачкаИсходящихДокументов">
		<xs:annotation>
			<xs:documentation>Абстрактный блок «ПачкаИсходящихДокументов».</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ВХОДЯЩАЯ_ОПИСЬ" type="ТипВХОДЯЩАЯ_ОПИСЬ" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Входящая опись. Отсутствуют при значении атрибута «Окружение» = «Единичный запрос»</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="СОПРОВОДИТЕЛЬНАЯ_ВЕДОМОСТЬ" type="ТипАДИ-5" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Исходящая опись по форме АДИ-5. Отсутствуют при значении атрибута «Окружение» = «Единичный запрос»</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

</xs:schema>