<?xml version="1.0" encoding="Windows-1251"?>
<xs:schema version="1.0"  xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">

	<xs:annotation>
		<xs:documentation xml:lang="ru">XML-подсхема для проверки документов - Типы данных</xs:documentation>
	</xs:annotation>

	<xs:complexType name="ТипДоверенность">
		<xs:sequence>						
			<xs:element name="Серия" minOccurs="0" type="xs:string">
				<xs:annotation>
					<xs:documentation>Серия доверенности</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Номер" minOccurs="0" type="xs:string">
				<xs:annotation>
					<xs:documentation>Номер доверенности</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ДатаВыдачи" type="xs:date">
				<xs:annotation>
					<xs:documentation>Дата выдачи доверенности</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="КемВыдана" type="xs:string">
				<xs:annotation>
					<xs:documentation>Кем выдана доверенность</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ДействуетС" type="xs:date">
				<xs:annotation>
					<xs:documentation>Дата с которой (включительно) действует доверенность</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ДействуетПо" type="xs:date">
				<xs:annotation>
					<xs:documentation>Дата по которую (включительно) действует доверенность</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<!--Тип имя файла -->
	<xs:simpleType name="ТипИмяФайла">
		<xs:restriction base="xs:string">
			<xs:pattern value="PFR-700-Y-\d{4}-ORG-\d{3}-\d{3}-\d{6}-DCK-\d{5}-DPT-\d{6}-DCK-\d{5}.XML"/>
		</xs:restriction>
	</xs:simpleType>

	<!--Тип Дата -->
	<xs:simpleType name="ТипДата">
		<xs:annotation>
			<xs:documentation>Дата в формате дд.мм.гггг</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">			
			<xs:pattern value="\d{2}\.\d{2}\.[1-2]\d{3}"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ТипДатаН">
		<xs:restriction base="xs:normalizedString">
			<xs:pattern value="\d{2}\.\d{2}\.\d{4}"/>
			<xs:pattern value=""/>
		</xs:restriction>
	</xs:simpleType>

	<!--Тип Дата Особая-->
	<xs:complexType name="ТипДатаОсобая">
		<xs:sequence>
			<xs:element name="День" minOccurs="0">				
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:minInclusive value="0"/>
						<xs:maxInclusive value="31"/>						
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Месяц" minOccurs="0">				
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:minInclusive value="0"/>
						<xs:maxInclusive value="12"/>						
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Год" type="xs:gYear"/>				
		</xs:sequence>
	</xs:complexType>

	<!--Тип ФИО -->
	<xs:complexType name="ТипФИО">
		<xs:annotation>
			<xs:documentation>Тип данных: ФИО физического лица</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Фамилия" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:normalizedString">						
						<xs:minLength value="1"/>
						<xs:maxLength value="40"/>						
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Имя" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:normalizedString">						
						<xs:minLength value="1"/>
						<xs:maxLength value="40"/>						
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Отчество" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:normalizedString">						
						<xs:maxLength value="40"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="ТипСоставДокументов">
		<xs:sequence>
			<xs:element name="Количество" type="ТипКоличество">
				<xs:annotation>
					<xs:documentation>Количество документов в составе</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="НаличиеДокументов" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Сведения о наличии документов определенного типа</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ТипДокумента" type="ТипДокумента">							
							<xs:annotation>
								<xs:documentation>Тип документа Количество документов определенного типа</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Количество" type="ТипКоличество">
							<xs:annotation>
								<xs:documentation>Количество документов определенного типа</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:simpleType name="ТипДокумента">
		<xs:annotation>
			<xs:documentation>Тип представленного документа</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="50"/>						
			<xs:enumeration value="АНКЕТА_ЗЛ"/>
			<xs:enumeration value="ЗАЯВЛЕНИЕ_ОБ_ОБМЕНЕ"/>
			<xs:enumeration value="ЗАЯВЛЕНИЕ_О_ДУБЛИКАТЕ"/>
			<xs:enumeration value="РАСПИСКА"/>
			<xs:enumeration value="ЗАПРОС_УТОЧНЕНИЯ_ЗАПОЛНЕННЫЙ"/>
			<xs:enumeration value="СВЕДЕНИЯ_О_СМЕРТИ"/>
			<xs:enumeration value="АКТУАЛИЗАЦИЯ_ЛС_ЗЛ"/>
			<xs:enumeration value="СТАРЫЕ_ИС_СЗВ-1"/>
			<xs:enumeration value="СТАРЫЕ_ИС_СЗВ-3"/>
			<xs:enumeration value="ИС_СЗВ-4-1"/>
			<xs:enumeration value="ИС_СЗВ-4-2"/>
			<xs:enumeration value="СВЕДЕНИЯ_О_СТРАХОВЫХ_ВЗНОСАХ_И_СТРАХОВОМ_СТАЖЕ_ЗЛ"/>
			<xs:enumeration value="СВЕДЕНИЯ_О_СУММЕ_ВЫПЛАТ_И_ВОЗНАГРАЖДЕНИЙ"/>
			<xs:enumeration value="ОПИСЬ_СВЕДЕНИЙ_ПЕРЕДАВАЕМЫХ_СТРАХОВАТЕЛЕМ"/>
			<xs:enumeration value="СВОДНАЯ_ВЕДОМОСТЬ"/>
			<xs:enumeration value="ВЕДОМОСТЬ_УПЛАТЫ"/>
			<xs:enumeration value="ЗАПРОС_ВЫПИСКИ_СОБЕСА"/>
			<xs:enumeration value="ЗАПРОС_ВЫПИСКИ_ЗЛ"/>
			<xs:enumeration value="КОНВЕРТАЦИЯ"/>
			<xs:enumeration value="СВЕДЕНИЯ_ОБ_ИНЫХ_ПЕРИОДАХ"/>
			<xs:enumeration value="ЗАЯВЛЕНИЕ_О_ВЫБОРЕ_УК"/>
			<xs:enumeration value="ЗАЯВЛЕНИЕ_О_ВЫБОРЕ_НПФ"/>
			<xs:enumeration value="УВЕДОМЛЕНИЕ_НПФ"/>
			<xs:enumeration value="ЗАЯВЛЕНИЕ_О_ПЕРЕХОДЕ_ИЗ_НПФ"/>
			<xs:enumeration value="ЗАЯВЛЕНИЕ_О_ПЕРЕХОДЕ_ИЗ_НПФ_УК"/>
			<xs:enumeration value="ЗАЯВЛЕНИЕ_О_СМЕНЕ_НПФ"/>
			<xs:enumeration value="АКТ_О_ВКЛЮЧЕНИИ_СВЕДЕНИЙ"/>
			<xs:enumeration value="ФИКСИРОВАННЫЙ_ПЛАТЕЖ"/>
			<xs:enumeration value="ЗАПРОС_СТРАХОВОГО_НОМЕРА"/>
			<xs:enumeration value="СВЕДЕНИЯ_О_НАЗНАЧЕНИИ_ПЕНСИИ"/>
			<xs:enumeration value="ВХОДЯЩАЯ_ОПИСЬ"/>
			<xs:enumeration value="ВХОДЯЩАЯ_ОПИСЬ_ПО_СТРАХОВЫМ_ВЗНОСАМ"/>
			<xs:enumeration value="ВХОДЯЩАЯ_ОПИСЬ_ПО_СУММАМ_ВЫПЛАТ"/>
			<xs:enumeration value="СТРАХОВОЕ_СВИДЕТЕЛЬСТВО"/>
			<xs:enumeration value="ЗАПРОС_УТОЧНЕНИЯ"/>
			<xs:enumeration value="ВЫПИСКА_ИЗ_ЛС"/>
			<xs:enumeration value="ОТКАЗ"/>
			<xs:enumeration value="СОПРОВОДИТЕЛЬНАЯ_ВЕДОМОСТЬ"/>
			<xs:enumeration value="РЕЕСТР_ДСВ_РАБОТОДАТЕЛЬ"/>
			<xs:enumeration value="ЗАЯВЛЕНИЕ_О_ВЫБОРЕ_СТРАХОВЩИКА"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ТипВходящегоДокумента">
		<xs:annotation>
			<xs:documentation>Тип входящего документа</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="50"/>
			<xs:enumeration value="АНКЕТА_ЗЛ"/>
			<xs:enumeration value="ЗАЯВЛЕНИЕ_ОБ_ОБМЕНЕ"/>
			<xs:enumeration value="ЗАЯВЛЕНИЕ_О_ДУБЛИКАТЕ"/>
			<xs:enumeration value="РАСПИСКА"/>
			<xs:enumeration value="ЗАПРОС_УТОЧНЕНИЯ_ЗАПОЛНЕННЫЙ"/>
			<xs:enumeration value="СВЕДЕНИЯ_О_СМЕРТИ"/>
			<xs:enumeration value="АКТУАЛИЗАЦИЯ_ЛС_ЗЛ"/>
			<xs:enumeration value="СТАРЫЕ_ИС_СЗВ-1"/>
			<xs:enumeration value="СТАРЫЕ_ИС_СЗВ-3"/>
			<xs:enumeration value="ИС_СЗВ-4-1"/>
			<xs:enumeration value="ИС_СЗВ-4-2"/>
			<xs:enumeration value="СВЕДЕНИЯ_О_СТРАХОВЫХ_ВЗНОСАХ_И_СТРАХОВОМ_СТАЖЕ_ЗЛ"/>
			<xs:enumeration value="СВЕДЕНИЯ_О_СУММЕ_ВЫПЛАТ_И_ВОЗНАГРАЖДЕНИЙ"/>
			<xs:enumeration value="ОПИСЬ_СВЕДЕНИЙ_ПЕРЕДАВАЕМЫХ_СТРАХОВАТЕЛЕМ"/>
			<xs:enumeration value="СВОДНАЯ_ВЕДОМОСТЬ"/>
			<xs:enumeration value="ВЕДОМОСТЬ_УПЛАТЫ"/>
			<xs:enumeration value="ЗАПРОС_ВЫПИСКИ_СОБЕСА"/>
			<xs:enumeration value="ЗАПРОС_ВЫПИСКИ_ЗЛ"/>
			<xs:enumeration value="КОНВЕРТАЦИЯ"/>
			<xs:enumeration value="СВЕДЕНИЯ_ОБ_ИНЫХ_ПЕРИОДАХ"/>
			<xs:enumeration value="ЗАЯВЛЕНИЕ_О_ВЫБОРЕ_УК"/>
			<xs:enumeration value="ЗАЯВЛЕНИЕ_О_ВЫБОРЕ_НПФ"/>
			<xs:enumeration value="УВЕДОМЛЕНИЕ_НПФ"/>
			<xs:enumeration value="ЗАЯВЛЕНИЕ_О_ПЕРЕХОДЕ_ИЗ_НПФ"/>
			<xs:enumeration value="ЗАЯВЛЕНИЕ_О_ПЕРЕХОДЕ_ИЗ_НПФ_УК"/>
			<xs:enumeration value="ЗАЯВЛЕНИЕ_О_СМЕНЕ_НПФ"/>
			<xs:enumeration value="АКТ_О_ВКЛЮЧЕНИИ_СВЕДЕНИЙ"/>
			<xs:enumeration value="ФИКСИРОВАННЫЙ_ПЛАТЕЖ"/>
			<xs:enumeration value="ЗАПРОС_СТРАХОВОГО_НОМЕРА"/>
			<xs:enumeration value="СВЕДЕНИЯ_О_НАЗНАЧЕНИИ_ПЕНСИИ"/>
			<xs:enumeration value="ВХОДЯЩАЯ_ОПИСЬ"/>
			<xs:enumeration value="ВХОДЯЩАЯ_ОПИСЬ_ПО_СТРАХОВЫМ_ВЗНОСАМ"/>
			<xs:enumeration value="ВХОДЯЩАЯ_ОПИСЬ_ПО_СУММАМ_ВЫПЛАТ"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="ТипНаличиеДокументов">
		<xs:sequence>
			<xs:element name="Количество" type="ТипКоличество">
				<xs:annotation>
					<xs:documentation>Количество документов в наличии</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="НаличиеДокументов" type="ТипНаличиеДокументов">
				<xs:annotation>
					<xs:documentation>Сведения о наличии документов</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>


	<xs:complexType name="ТипФИОН">
		<xs:sequence>
			<xs:element name="Фамилия" minOccurs="0">				
				<xs:simpleType>
					<xs:restriction base="xs:normalizedString">
						<xs:maxLength value="40"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Имя" minOccurs="0">				
				<xs:simpleType>
					<xs:restriction base="xs:normalizedString">
						<xs:maxLength value="40"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Отчество" minOccurs="0">				
				<xs:simpleType>
					<xs:restriction base="xs:normalizedString">
						<xs:maxLength value="40"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>


	<xs:simpleType name="ТипПол">
		<xs:restriction base="ТипПолН">
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ТипПолН">
		<xs:restriction base="xs:normalizedString">
			<xs:enumeration value="МУЖСКОЙ"/>
			<xs:enumeration value="ЖЕНСКИЙ"/>
			<xs:enumeration value="М"/>
			<xs:enumeration value="Ж"/>
			<xs:enumeration value=""/>
		</xs:restriction>
	</xs:simpleType>


	<!--Тип СтраховойНомер -->	

	<xs:simpleType name="ТипСтраховойНомер">
		<xs:restriction base="xs:normalizedString">
			<xs:pattern value="\d{3}-\d{3}-\d{3} \d{2}"/>
			<xs:pattern value="\d{3}-\d{3}-\d{3}-\d{2}"/>			
		</xs:restriction>
	</xs:simpleType>

	<!--Тип Регистрационный Номер -->
	<xs:simpleType name="ТипРегНомер">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{3}-\d{3}-\d{6}"/>
		</xs:restriction>
	</xs:simpleType>

	<!--Тип КПП -->
	<xs:simpleType name="ТипКПП">
		<xs:restriction base="xs:normalizedString">
			<xs:pattern value="\d{9}"/>
		</xs:restriction>
	</xs:simpleType>

	<!--Тип НН налоговый номер -->
	<xs:complexType name="ТипНН">
		<xs:sequence>
			<xs:element name="ИНН" type="ТипИНН"/>
			<xs:element name="КПП" type="ТипКПП" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>


	<!--Тип Номер документа в пачке 1..999 -->
	<xs:simpleType name="ТипНомерВПачке">
		<xs:restriction base="xs:positiveInteger">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="999"/>
			<xs:totalDigits value="3"/>
		</xs:restriction>
	</xs:simpleType>



	<!--Тип дата для печати -->
	<xs:simpleType name="Тип21">
		<xs:restriction base="xs:string">
			<xs:minLength value="8"/>
			<xs:maxLength value="21"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Тип4">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="4"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Тип50">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="50"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Тип150">
		<xs:restriction base="xs:string">
			<xs:maxLength value="150"/>
		</xs:restriction>
	</xs:simpleType>


	<xs:simpleType name="Тип5">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="5"/>
		</xs:restriction>
	</xs:simpleType>

	<!--Тип Наименование портфеля -->
	<xs:simpleType name="Тип100Н">
		<xs:restriction base="xs:normalizedString">
			<xs:maxLength value="100"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ТипНомер">
		<xs:restriction base="xs:normalizedString">
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>


	<xs:simpleType name="Тип200Н">
		<xs:restriction base="xs:normalizedString">
			<xs:maxLength value="200"/>
		</xs:restriction>
	</xs:simpleType>

	<!--Тип Номер пачки -->
	<xs:simpleType name="ТипНомерПачки">
		<xs:restriction base="xs:positiveInteger">
			<xs:maxInclusive value="99999"/>
			<xs:totalDigits value="5"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="ТипНомерПачкиВнешний">
		<xs:choice>
			<xs:element name="Основной" type="ТипНомерПачки"/>
			<xs:element name="ПоПодразделению" type="ТипНомерПачки"/>
		</xs:choice>
	</xs:complexType>
	<xs:simpleType name="Тип40">
		<xs:restriction base="xs:normalizedString">
			<xs:minLength value="1"/>
			<xs:maxLength value="40"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Тип30">
		<xs:restriction base="xs:normalizedString">
			<xs:minLength value="1"/>
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Тип200">
		<xs:restriction base="xs:normalizedString">
			<xs:minLength value="1"/>
			<xs:maxLength value="200"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ТипСостояниеЭлемента">
		<xs:restriction base="xs:string">
			<xs:maxLength value="4"/>
			<xs:enumeration value="АКТЛ"/>
			<xs:enumeration value="УПРЗ"/>
			<xs:enumeration value="ПОЛН"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ТипКоличество3">
		<xs:restriction base="xs:unsignedLong">
			<xs:totalDigits value="3"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ТипКоличество12">
		<xs:restriction base="xs:unsignedLong">
			<xs:totalDigits value="12"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ТипКоличество10">
		<xs:restriction base="xs:unsignedLong">
			<xs:totalDigits value="10"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ТипКоличество9">
		<xs:restriction base="xs:unsignedLong">
			<xs:totalDigits value="9"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Тип1000">
		<xs:restriction base="xs:normalizedString">
			<xs:minLength value="1"/>
			<xs:maxLength value="1000"/>
		</xs:restriction>
	</xs:simpleType>


	<xs:simpleType name="Тип40Н">
		<xs:restriction base="xs:normalizedString">
			<xs:maxLength value="40"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Тип30Н">
		<xs:restriction base="xs:normalizedString">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>


	<!-- Тип Подразделение -->
	<xs:complexType name="ТипПодразделение">
		<xs:sequence>
			<xs:element name="НаименованиеПодразделения" type="Тип100Н"/>
			<xs:element name="НомерПодразделения">				
				<xs:simpleType>
					<xs:restriction base="xs:normalizedString">
						<xs:minLength value="1"/>
						<xs:maxLength value="6"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

