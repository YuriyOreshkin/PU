			    </xsd:simpleType>
			</xsd:element>
			<xsd:element name="НазваниеДополнительногоТарифа" minOccurs="0">
			    <xsd:simpleType>
			        <xsd:restriction base="xsd:normalizedString">
			            <xsd:maxLength value="20"/>
			            <xsd:enumeration value="ЧЛЕН ЛЭВС"/>
			            <xsd:enumeration value=""/>
