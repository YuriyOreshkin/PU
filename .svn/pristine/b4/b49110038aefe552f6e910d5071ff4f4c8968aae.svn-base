    <xsd:complexType name="ТипЗаработок">
        <xsd:sequence>
            <xsd:element name="ТипСтроки">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:normalizedString">
                        <xsd:enumeration value="ИТОГ"/>
                        <xsd:enumeration value="МЕСЦ"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="Год" type="xsd:gYear" minOccurs="0"/>
            <xsd:element name="Месяц" minOccurs="0">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:unsignedLong">
                        <xsd:minInclusive value="1"/>
                        <xsd:maxInclusive value="12"/>
                        <xsd:totalDigits value="2"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="НачисленоВсего" type="pfr:ТипДеньги" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>


	<xsd:complexType name="ТипВсеПериодыРаботы">
		<xsd:sequence maxOccurs="1">
			<xsd:element name="Количество" maxOccurs="1" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="99"/>
						<xsd:totalDigits value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="ПериодРаботы" maxOccurs="99" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="ОсновнаяСтрока">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="НомерСтроки">
										<xsd:simpleType>
											<xsd:restriction base="xsd:integer">
												<xsd:minInclusive value="1"/>
												<xsd:totalDigits value="3"/>
												<xsd:maxInclusive value="99"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="ДатаНачалаПериода" type="ТипДата"/>
									<xsd:element name="ДатаКонцаПериода" type="ТипДата"/>
									<xsd:element name="ОсобенностиУчета" type="ТипОсобенностиУчета" minOccurs="0"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="ДополнительныеСтроки" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="Количество">
										<xsd:simpleType>
											<xsd:restriction base="xsd:integer">
												<xsd:minInclusive value="1"/>
												<xsd:maxInclusive value="10"/>
												<xsd:totalDigits value="2"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="ОсобенностиУчета" type="ТипОсобенностиУчета" maxOccurs="10"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>


	<xsd:complexType name="ТипОтчетныйПериод">
	    <xsd:sequence>
	        <xsd:element name="Квартал">
	            <xsd:simpleType>
	                <xsd:restriction base="xsd:integer">
	                    <xsd:totalDigits value="1"/>
					</xsd:restriction>
	            </xsd:simpleType>
	        </xsd:element>
	        <xsd:element name="Год">
	            <xsd:simpleType>
	                <xsd:restriction base="xsd:gYear">
	                    <xsd:minInclusive value="1996"/>
	                    <xsd:maxInclusive value="2001"/>
	                </xsd:restriction>
	            </xsd:simpleType>
	        </xsd:element>
	        <xsd:element name="Название" minOccurs="0" type="pfr:Тип30Н"/>
	    </xsd:sequence>
	</xsd:complexType>


	<xsd:complexType name="ТипДоговор">
	    <xsd:sequence>
	        <xsd:element name="Номер" type="pfr:ТипНомер" minOccurs="0"/>
	        <xsd:element name="ДатаЗаключения" type="pfr:ТипДата"/>
	        <xsd:element name="КодВидаВыплаты">
	            <xsd:simpleType>
	                <xsd:restriction base="xsd:normalizedString">
	                    <xsd:maxLength value="6"/>
	                    <xsd:enumeration value="АВАНС"/>
	                    <xsd:enumeration value="ПРОЧЕЕ"/>
	                </xsd:restriction>
	            </xsd:simpleType>
	        </xsd:element>
	        <xsd:element name="НазваниеВидаВыплаты" minOccurs="0">
	            <xsd:simpleType>
	                <xsd:restriction base="xsd:normalizedString">
	                    <xsd:maxLength value="20"/>
	                </xsd:restriction>
	            </xsd:simpleType>
	        </xsd:element>
	    </xsd:sequence>
	</xsd:complexType>


	<xsd:complexType name="ТипСтрокаЗаработка">
	    <xsd:sequence>
	        <xsd:element name="ТипСтроки">
	            <xsd:simpleType>
	                <xsd:restriction base="xsd:normalizedString">
	                    <xsd:enumeration value="МЕСЯЦ"/>
	                    <xsd:enumeration value="ИТОГО"/>
	                    <xsd:maxLength value="5"/>
	                </xsd:restriction>
	            </xsd:simpleType>
	        </xsd:element>
	        <xsd:element name="Месяц" minOccurs="0">
	            <xsd:simpleType>
	                <xsd:restriction base="xsd:integer">
	                    <xsd:minInclusive value="1"/>
	                    <xsd:maxInclusive value="12"/>
	                    <xsd:totalDigits value="2"/>
					</xsd:restriction>
	            </xsd:simpleType>
	        </xsd:element>
	        <xsd:element name="НачисленоВсего" type="pfr:ТипДеньги"/>
	        <xsd:element name="НачисленоПоБольничным" type="pfr:ТипДеньги" minOccurs="0"/>
	    </xsd:sequence>
	</xsd:complexType>


</xsd:schema>

