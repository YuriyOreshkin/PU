			            <xsd:maxLength value="3"/>
			            <xsd:enumeration value="РКС"/>
			            <xsd:enumeration value="МКС"/>
			            <xsd:enumeration value="Ч31"/>
			            <xsd:enumeration value="Ч33"/>
			            <xsd:enumeration value="Ч34"/>
			            <xsd:enumeration value="Ч35"/>
			            <xsd:enumeration value="Ч36"/>
