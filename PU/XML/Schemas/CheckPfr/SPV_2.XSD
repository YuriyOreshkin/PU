<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:annotation>
		<xsd:documentation xml:lang="ru">XML-подсхема для проверки СПВ-1</xsd:documentation>
	</xsd:annotation>
	<xsd:include schemaLocation="Types.XSD"/>
	<xsd:include schemaLocation="Types_INDS.XSD"/>
	<xsd:include schemaLocation="Types_SPV.XSD"/>
	<xsd:complexType name="СуммаВыплатИвознагражденийПоДопТарифу">
		<xsd:sequence>
			<xsd:element name="ТипСтроки">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="МЕСЦ"/>
						<xsd:enumeration value="ИТОГ"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="Месяц" type="ТипМесяц" minOccurs="0"/>
			<xsd:element name="СуммаВыплатПоДопТарифу27-1" type="ТипДеньги" minOccurs="0"/>
			<xsd:element name="СуммаВыплатПоДопТарифу27-2-18" type="ТипДеньги" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="ТипПризнакУплаты">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ДА"/>
			<xsd:enumeration value="НЕТ"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="СуммаВыплатИвознагражденийВпользуЗЛ">
		<xsd:sequence>
			<xsd:element name="ТипСтроки">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="МЕСЦ"/>
						<xsd:enumeration value="ИТОГ"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="Месяц" type="ТипМесяц" minOccurs="0"/>
			<xsd:element name="СуммаВыплатВсего" type="ТипДеньги" minOccurs="0"/>
			<xsd:element name="СуммаВыплатНачисленыСтраховыеВзносыНеПревышающие" type="ТипДеньги" minOccurs="0"/>
			<xsd:element name="СуммаВыплатНачисленыСтраховыеВзносыПревышающие" type="ТипДеньги" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ВХОДЯЩАЯ_ОПИСЬ">
		<xsd:sequence>
			<xsd:element name="НомерВпачке">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="1"/>
						<xsd:maxInclusive value="32001"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="ТипВходящейОписи">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="ОПИСЬ ПАЧКИ"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="СоставительПачки" type="ТипОрганизация"/>
			<xsd:element name="НомерПачки">
				<xsd:complexType>
					<xsd:choice>
						<xsd:element name="Основной" type="ТипНомерПачки"/>
						<xsd:element name="ПоПодразделению" type="ТипНомерПачки"/>
					</xsd:choice>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="СоставДокументов">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Количество">
							<xsd:simpleType>
								<xsd:restriction base="xsd:integer">
									<xsd:enumeration value="1"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="НаличиеДокументов">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="ТипДокумента">
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:enumeration value="СВЕДЕНИЯ_О_СТРАХОВОМ_СТАЖЕ_ЗЛ_ДЛЯ_УСТАНОВЛЕНИЯ_ПЕНСИИ"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="Количество">
										<xsd:simpleType>
											<xsd:restriction base="xsd:positiveInteger">
												<xsd:maxInclusive value="32766"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
								</xsd:sequence>
								<xsd:attribute name="Вариант" type="xsd:string"/>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="ДатаСоставления" type="ТипДата"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- Тип ВхДок-->
	<xsd:complexType name="СВЕДЕНИЯ_О_СТРАХОВОМ_СТАЖЕ_ЗЛ_ДЛЯ_УСТАНОВЛЕНИЯ_ПЕНСИИ">
		<xsd:sequence>
			<xsd:element name="НомерВпачке">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="1"/>
						<xsd:maxInclusive value="32001"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="ТипСведений" type="ТипТипСведенийСПВ"/>
			<xsd:element name="РегистрационныйНомер" type="ТипРегНомер"/>
			<xsd:element name="НаименованиеКраткое">
				<xsd:simpleType>
					<xsd:restriction base="Тип100">
						<xsd:maxLength value="50"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="НалоговыйНомер" type="ТипНН"/>
			<xsd:element name="КодКатегории" type="ТипКодКатегорииСПВ2"/>
			<xsd:element name="ОтчетныйПериод" type="ТипОтчетныйПериодСПВ2"/>
			<xsd:element name="КорректируемыйОтчетныйПериод" type="ТипОтчетныйПериодСПВ2" minOccurs="0"/>
			<xsd:element name="СтраховойНомер" type="ТипСтраховойНомер"/>
			<xsd:element name="ФИО" type="ТипФИО"/>
			<xsd:element name="ДатаЗаполнения" type="ТипДата"/>
			<xsd:element name="ДатаСоставленияНа" type="ТипДата"/>
			<xsd:element name="ДатаПредставленияВПФР" type="ТипДата" minOccurs="0"/>
			<xsd:element name="СтажевыйПериод" type="ТипСтажевыйПериодРСВ" minOccurs="0" maxOccurs="99"/>
			<xsd:element name="ПризнакНачисленияВзносовОПС" type="ТипПризнакУплаты" minOccurs="0"/>
			<xsd:element name="ПризнакНачисленияВзносовПоДопТарифу" type="ТипПризнакУплаты" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ВходящийДокумент  -->
	<!-- ВходящийДокумент  -->
	<xsd:element name="ФайлПФР">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="ИмяФайла" type="ТипИмяФайла"/>
				<xsd:element name="ЗаголовокФайла">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="ВерсияФормата">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="07.00"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="ТипФайла">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="ВНЕШНИЙ"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="ПрограммаПодготовкиДанных">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="НазваниеПрограммы" type="Тип40"/>
										<xsd:element name="Версия">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:minLength value="1"/>
													<xsd:maxLength value="10"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="ИсточникДанных">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="СТРАХОВАТЕЛЬ"/>
										<xsd:enumeration value="ТРАНСФЕР-АГЕНТ"/>
										<xsd:enumeration value="ОПФР-ЛИЧНО"/>
										<xsd:enumeration value="ОПФР-ПРОЧЕЕ"/>
										<xsd:enumeration value="НПФ"/>
										<xsd:enumeration value="ПФР"/>
										<xsd:enumeration value="ОРГАН_ЗАГС"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="ПачкаВходящихДокументов">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="ВХОДЯЩАЯ_ОПИСЬ" type="ВХОДЯЩАЯ_ОПИСЬ"/>
							<xsd:choice>
								<xsd:element name="СВЕДЕНИЯ_О_СТРАХОВОМ_СТАЖЕ_ЗЛ_ДЛЯ_УСТАНОВЛЕНИЯ_ПЕНСИИ" type="СВЕДЕНИЯ_О_СТРАХОВОМ_СТАЖЕ_ЗЛ_ДЛЯ_УСТАНОВЛЕНИЯ_ПЕНСИИ" maxOccurs="32766"/>
							</xsd:choice>
						</xsd:sequence>
						<xsd:attribute name="Окружение" use="required">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:enumeration value="Единичный запрос"/>
									<xsd:enumeration value="В составе файла"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
						<xsd:attribute name="Стадия" use="optional">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:enumeration value="До обработки"/>
									<xsd:enumeration value="После обработки"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
