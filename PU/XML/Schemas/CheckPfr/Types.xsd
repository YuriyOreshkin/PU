	<xsd:simpleType name="ТипКодПодразделения">
		<xsd:restriction base="xsd:unsignedLong">
			<xsd:totalDigits value="6"/>
			<xsd:maxInclusive value="999999"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="ТипСотрудникПодразделения">
		<xsd:sequence>
			<xsd:element name="КодПодразделения" type="ТипКодПодразделения" minOccurs="0"/>
