	<xsd:simpleType name="ТипУдостоверитель">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="20"/>
			<xsd:enumeration value="НОТАРИУС"/>
			<xsd:enumeration value="КОНСУЛ"/>
			<xsd:enumeration value="ГОСПИТАЛЬ"/>
			<xsd:enumeration value="ВОЕННАЯ ЧАСТЬ"/>
			<xsd:enumeration value="УИН"/>
			<xsd:enumeration value="УСЗН"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ТипЧислоЗЛ">
		<xsd:restriction base="xsd:unsignedLong">
			<xsd:totalDigits value="6"/>
			<xsd:minInclusive value="0"/>
			<xsd:maxInclusive value="999999"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ТипКоличество">
		<xsd:restriction base="xsd:integer">
			<xsd:minInclusive value="0"/>
			<xsd:maxInclusive value="9999"/>
			<xsd:totalDigits value="4"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ТипДеньги">
		<xsd:restriction base="xsd:decimal">
			<xsd:fractionDigits value="2"/>
			<xsd:totalDigits value="15"/>
			<xsd:pattern value="\-?[0-9]+\.\d{2}|\-?[0-9]+"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ТипДеньгиЦелые">
		<xsd:restriction base="xsd:long">
			<xsd:totalDigits value="13"/>
			<xsd:minInclusive value="-9999999999999"/>
			<xsd:maxInclusive value="9999999999999"/>
		</xsd:restriction>
	</xsd:simpleType>
</xsd:schema>
