					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="НазваниеКатегории" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:normalizedString">
						<xs:maxLength value="20"/>
