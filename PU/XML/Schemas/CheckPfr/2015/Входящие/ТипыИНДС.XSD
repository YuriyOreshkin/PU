				</xs:simpleType>
			</xs:element>
			<xs:element name="НазваниеДополнительногоТарифа" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:normalizedString">
						<xs:maxLength value="20"/>
						<xs:enumeration value="ЧЛЕН ЛЭВС"/>
