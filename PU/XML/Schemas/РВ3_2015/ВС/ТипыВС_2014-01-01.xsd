<?xml version="1.0" encoding="Windows-1251"?>
<xs:schema version="2.0" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://пф.рф/ВС/типы/2014-01-01" xmlns:АФ="http://пф.рф/АФ" targetNamespace="http://пф.рф/ВС/типы/2014-01-01"
           elementFormDefault="qualified">

    <xs:import namespace="http://пф.рф/АФ" schemaLocation="../ТипыОбщие.xsd"/>

    <xs:annotation>
        <xs:documentation>Общие типы данных для информационного обмена "Взаимодействие со страхователями"</xs:documentation>
        <xs:documentation>Рекомендуемый префикс для пространства имен http://пф.рф/ВС/типы/2014-01-01 - ВС</xs:documentation>
    </xs:annotation>

    <xs:complexType name="ТипЗаголовокПакетаВС">
        <xs:annotation>
            <xs:documentation>Тип заголовка пакета для документов ИО "Взаимодействие со страхователями"</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="АФ:ТипЗаголовокПакета">
                <xs:sequence/>
                <xs:attribute name="ИсточникДанных" type="xs:string" use="required">
                    <xs:annotation>
                        <xs:documentation>Составитель файла</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="СпособПередачи" type="xs:string" use="optional">
                    <xs:annotation>
                        <xs:documentation>Способ передачи документа</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:simpleType name="ТипКодОтчПериода">
        <xs:annotation>
            <xs:documentation>Тип, используемый для представления кода отчетного периода : 3 - 1 квартал, 6 - полугодие, 9 - 9 месяцев, 0 - год</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:enumeration value="3"/>
            <xs:enumeration value="6"/>
            <xs:enumeration value="9"/>
            <xs:enumeration value="0"/>
        </xs:restriction>
    </xs:simpleType>

</xs:schema>